{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "credit.checked data",
  "type": "object",
  "required": [
    "checkId",
    "customerId",
    "type",
    "result",
    "checkedAt",
    "provider"
  ],
  "properties": {
    "checkId": {
      "type": "string",
      "format": "uuid"
    },
    "leadId": {
      "type": "string",
      "format": "uuid"
    },
    "customerId": {
      "type": "string",
      "format": "uuid"
    },
    "type": {
      "type": "string",
      "enum": [
        "PRECHECK",
        "FINAL"
      ]
    },
    "result": {
      "type": "string",
      "enum": [
        "APPROVED",
        "PENDING",
        "DECLINED"
      ]
    },
    "riskScore": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "limitAmount": {
      "type": "number",
      "minimum": 0
    },
    "currency": {
      "type": "string",
      "minLength": 3,
      "maxLength": 3
    },
    "provider": {
      "type": "string",
      "enum": [
        "ALLIANZ",
        "XENTRAL",
        "OTHER"
      ]
    },
    "checkedAt": {
      "type": "string",
      "format": "date-time"
    },
    "ttlSeconds": {
      "type": "integer",
      "minimum": 0
    },
    "meta": {
      "type": "object"
    }
  },
  "additionalProperties": false
}